{
    "extends": [
        "eslint-config-airbnb",
        "prettier"
    ],
    "overrides": [
        {
            "files": [
                "**/*.ts",
                "**/*.tsx"
            ],
            "rules": {
                "no-multiple-empty-lines": [
                    "error",
                    {
                        "max": 1,
                        "maxBOF": 1
                    }
                ],
                "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".jsx"] }]
            },
            "plugins": [
                "@typescript-eslint",
            ],
            "extends": [
                "eslint:recommended",
                "plugin:@typescript-eslint/recommended"
            ],
            "parser": "@typescript-eslint/parser",
            "parserOptions": {
                "project": [
                    "./tsconfig.json"
                ],
            },
        },
    ],
    "plugins": [
      "prettier"
    ],
}